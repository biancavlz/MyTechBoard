<h1>Articles</h1>

{{#each data.articles}}
  {{#if this.title}}
    <h3>{{this.title}}</h3>
    <a href="{{this.link}}" target='_blank'>read more</a>
  {{else}}
    <a href="{{this.link}}" target='_blank'>{{this.link}}</a>
  {{/if}}
{{/each}}


<h1>News</h1>

{{#each data.news}}
  {{#if this.title}}
    <h3>{{this.title}}</h3>
    <a href="{{this.link}}" target='_blank'>read more</a>
  {{else}}
    <a href="{{this.link}}" target='_blank'>{{this.link}}</a>
  {{/if}}
{{/each}}
